{
    "manifest_version": 2,
    "name": "XML Formatter",
    "version": "1.0",
    "description": "Formats and pretty-prints XML.",
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Format XML",
      "default_popup": "popup.html",
      "default_height": 600
    },
    "permissions": ["activeTab", "clipboardWrite"]
}
  